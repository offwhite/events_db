%table.list
  - profiles.each do |profile|
    - present profile do |profile|
      %tr
        %td
          = link_to profile.title, profile_home_path(username: profile.username)
        %td
          = profile.role_summary
        %td
          = profile.town_titles_sml
        %td.text-right
          = "#{profile.events.count} events"
